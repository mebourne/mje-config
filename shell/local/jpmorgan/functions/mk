#eval compdef -n _make mk
# ZSH function file
# Make part of the cashsec system
# Usage: make [<options>]
#
# $Id$

if [[ $PKGGCCHOME == */4.1.0 ]]
then
  clearmake -f /vobs/odyssey/shared/config/makemakefile_gcc4 &&
    PATH=$PATH:. clearmake "$@"
else
  clearmake -f /vobs/odyssey/shared/config/makemakefile &&
    PATH=$PATH:. clearmake "$@"
fi
